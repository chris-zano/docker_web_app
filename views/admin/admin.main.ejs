<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="/files/favicon/" type="image/x-icon">
    <link rel="stylesheet" href="/files/css/root/root.css">

    <% for( let stylesheet_url of stylesheets_urls) { %>
    <link rel="stylesheet" href="<%= stylesheet_url %>">
    <% } %>
    <link rel="stylesheet" href="/files/css/root/search.css">

    <title>(AT-FS): [Admin] <%= pageUrl %></title>
</head>
<body>
    <%- include('templates/admin.header.ejs') %>
    
    <main>
        
        <% if (pageUrl === "dashboard") { %>
            <%- include('layouts/admin.home.ejs') %>
        <% } else if (pageUrl === "uploads") { %>
            <%- include('layouts/admin.upload.ejs') %>
        <% } else if (pageUrl === "search") { %>
            <%- include('../search.ejs') %>
        <% } else if (pageUrl === "profile") { %>
            <%- include('layouts/admin.profile.ejs') %>
        <% } else if (pageUrl === "search-result") { %>
            <%- include('layouts/admin.home.ejs') %>
        <% } %>
    </main>

    <%- include('templates/admin.scripts.ejs') %>
</body>
</html>